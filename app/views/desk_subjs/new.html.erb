<h1>New desk_subj</h1>

<% form_for(@desk_subj) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :subj %><br />
    <%= f.text_area :subj %>
  </p>
  <p>
    <%= f.label :s_text %><br />
    <%= f.text_area :s_text %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', desk_subjs_path %>
