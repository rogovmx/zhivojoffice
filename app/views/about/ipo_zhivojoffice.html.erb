      <%= f.label 'E-mail *' %><br />
      <%= f.text_field :email , :style=>'width: 300px; margin-bottom: 10px;' %>
      <%= f.label 'Телефон (с кодом) *' %><br />
      <%= f.text_field :phone , :style=>'width: 300px; margin-bottom: 10px;' %>
